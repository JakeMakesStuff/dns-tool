{
  "name": "dns-tool",
  "version": "1.0.0",
  "description": "A simple browser-based tool to perform DNS lookups.",
  "license": "Apache-2.0",
  "scripts": {
    "build": "parcel build ./src/index.html --public-url=./",
    "dev": "parcel ./src/index.html",
    "lint": "tsc && eslint ./src/**/*.ts && eslint ./src/**/*.vue",
    "lint-and-fix": "tsc && eslint ./src/**/*.ts --fix && eslint ./src/**/*.vue --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/do-community/dns-tool.git"
  },
  "keywords": [
    "dns"
  ],
  "author": "DigitalOcean",
  "bugs": {
    "url": "https://github.com/do-community/dns-tool/issues"
  },
  "homepage": "https://github.com/do-community/dns-tool#readme",
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "babel-polyfill": "^6.26.0",
    "parcel-bundler": "1.12.3",
    "vue": "^2.6.10",
    "vue-hot-reload-api": "^2.3.3",
    "vue-tippy": "^4.0.0-beta.4"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "eslint-plugin-vue": "5.2.3",
    "@vue/component-compiler-utils": "^3.0.0",
    "eslint": "^6.1.0",
    "typescript": "^3.5.3",
    "vue-template-compiler": "^2.6.10"
  }
}
